{
  "name": "vue-admin-template",
  "version": "1.0.0",
  "license": "MIT",
  "description": "A vue admin template with Element UI & axios & iconfont & permission control & lint",
  "author": "Pan <panfree23@gmail.com>",
  "scripts": {
    "dev": "cross-env ENV_API=dev webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "dev:test": "cross-env ENV_API=test webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "dev:pre": "cross-env ENV_API=pre webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "build": "cross-env BUILD_FOLDER=dist node build/build.js",
    "build:dev": "cross-env BUILD_FOLDER=dev node build/build.js",
    "build:test": "cross-env BUILD_FOLDER=test node build/build.js",
    "build:pre": "cross-env BUILD_FOLDER=pre node build/build.js",
    "build:report": "npm_config_report=true npm run build",
    "start": "node build/dev-server.js",
    "lint": "eslint --ext .js,.vue src",
    "test": "node build/build-test.js"
  },
